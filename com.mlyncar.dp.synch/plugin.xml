<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Sample Action Set"
            visible="true"
            id="com.mlyncar.dp.synch.actionSet">
         <menu
               label="Synchronization &amp;Menu"
               id="synchMenu">
            <separator
                  name="synchGroup">
            </separator>
         </menu>
         <action
               label="&amp;Synch Action"
               icon="icons/sample.gif"
               class="com.mlyncar.dp.synch.actions.SynchAction"
               tooltip="Synchronize sequence diagram and source code"
               menubarPath="synchMenu/synchGroup"
               toolbarPath="synchGroup"
               id="com.mlyncar.dp.synch.actions.SynchAction">
         </action>
      </actionSet>
   </extension>

</plugin>
