<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Synchronization Action Set"
            visible="true"
            id="com.mlyncar.dp.analyzer.actionSet">
         <menu
               label="Synchronization &amp;Menu"
               id="synchMenu">
            <separator
                  name="synchGroup">
            </separator>
         </menu>
         <action
               label="&amp;Synchronize Diagrams"
               icon="icons/sample.gif"
               class="com.mlyncar.dp.analyzer.actions.AnalyzeAction"
               tooltip="Execute diagram synchronization"
               menubarPath="synchMenu/synchGroup"
               toolbarPath="synchGroup"
               id="com.mlyncar.dp.analyzer.actions.AnalyzeAction">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            commandId="com.mlyncar.dp.analyzer.handler1">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
   </extension>

</plugin>
